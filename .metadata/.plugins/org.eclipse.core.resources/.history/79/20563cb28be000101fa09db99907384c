/* ===============================================================
 * HÀM ĐIỀU KHIỂN MOTOR
 * =============================================================== */
void Motor_Control(float pwm_input)
{
    if (pwm_input > 4199.0f) pwm_input = 4199.0f;
    else if (pwm_input < -4199.0f) pwm_input = -4199.0f;

    int pwm_val = (int)fabs(pwm_input);

    if (pwm_input > 0) // Forward
    {
        HAL_GPIO_WritePin(GPIOA, GPIO_PIN_10, 1);
        HAL_GPIO_WritePin(GPIOA, GPIO_PIN_9, 0);

        HAL_GPIO_WritePin(GPIOA, GPIO_PIN_8, 1);
        HAL_GPIO_WritePin(GPIOC, GPIO_PIN_9, 0);
    }
    else if (pwm_input < 0) // Backward
    {
        HAL_GPIO_WritePin(GPIOA, GPIO_PIN_10, 0); //AIN2
        HAL_GPIO_WritePin(GPIOA, GPIO_PIN_9, 1); //AIN1

        HAL_GPIO_WritePin(GPIOA, GPIO_PIN_8, 0); //BIN1
        HAL_GPIO_WritePin(GPIOC, GPIO_PIN_9, 1); //BIN2
    }
    else // Stop
    {
        pwm_val = 0;
    }

    __HAL_TIM_SET_COMPARE(&htim2, TIM_CHANNEL_2, pwm_val);
    __HAL_TIM_SET_COMPARE(&htim2, TIM_CHANNEL_3, pwm_val);
}

/*
#define ADD 0xD0
#define RTD 57.2957

int16_t ax = 0, ay = 0, az = 0, gx = 0, gy = 0, gz = 0;
float AX, AY, AZ, GX, GY, GZ;
float pitch = 0, roll = 0, prev_pitch = 0;
float filtered_pitch = 0;

void MPU6050Init(void)
{
	uint8_t check;
	uint8_t mData;
	HAL_I2C_Mem_Read(&hi2c1, ADD, 0x75, 1, &check, 1, 1000);
	if(check == 0x68)
	{
		mData = 0x00;
		HAL_I2C_Mem_Write(&hi2c1, ADD, 0x6B, 1, &mData, 1, 1000);
		mData = 0x07;
		HAL_I2C_Mem_Write(&hi2c1, ADD, 0x19, 1, &mData, 1, 1000);
		mData = 0x00;
		HAL_I2C_Mem_Write(&hi2c1, ADD, 0x1B, 1, &mData, 1, 1000);
		mData = 0x00;
		HAL_I2C_Mem_Write(&hi2c1, ADD, 0x1C, 1, &mData, 1, 1000);
	}
}

void MPU6050ReadG(void)
{
	uint8_t dataG[6];
	HAL_I2C_Mem_Read(&hi2c1, ADD, 0x43, 1, dataG, 6, 1000);
	gx = (int16_t) (dataG[0] << 8 | dataG[1]);
	gy = (int16_t) (dataG[2] << 8 | dataG[3]);
	gz = (int16_t) (dataG[4] << 8 | dataG[5]);
	GX =(float) gx / 131.0;
	GY =(float) gy / 131.0;
	GZ =(float) gz / 131.0;

}

void MPU6050ReadA(void)
{
	uint8_t dataA[6];
	HAL_I2C_Mem_Read(&hi2c1, ADD, 0x3B, 1, dataA, 6, 1000);
	ax = (int16_t) (dataA[0] << 8 | dataA[1]);
	ay = (int16_t) (dataA[2] << 8 | dataA[3]);
	az = (int16_t) (dataA[4] << 8 | dataA[5]);
	AX = (float) ax / 16384.0;
	AY = (float) ay / 16384.0;
	AZ = (float) az / 16384.0;

}

void MPU6050_getAngle(float AX, float AY, float AZ, float GX, float GY, float GZ)
{
	float pitchG = pitch + GX * (10000 / 1000000.0f);
	float rollG = roll + GY * (10000 / 1000000.0f);

	float pitchA = atan2(AY, sqrt(AX * AX + AZ * AZ)) * RTD;
	float rollA = atan2(AX, sqrt(AY * AY + AZ * AZ)) * RTD;

	pitch = 0.98 * pitchG + 0.02 * pitchA;
	roll = 0.98 * rollG + 0.02 * rollA;
}

void MPU6050_filter(float pitch, float GX, float dt)
{
	filtered_pitch = 0.98f * (prev_pitch + GX * dt) + 0.02 * pitch;
	prev_pitch = filtered_pitch;
}
*/

