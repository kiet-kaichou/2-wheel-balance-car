/**
  * @brief GPIO Initialization Function
  * @param None
  * @retval None
  */
static void MX_GPIO_Init(void)
{
  GPIO_InitTypeDef GPIO_InitStruct = {0};
  /* USER CODE BEGIN MX_GPIO_Init_1 */

  /* USER CODE END MX_GPIO_Init_1 */

  /* GPIO Ports Clock Enable */
  __HAL_RCC_GPIOH_CLK_ENABLE();
  __HAL_RCC_GPIOA_CLK_ENABLE();
  __HAL_RCC_GPIOE_CLK_ENABLE();
  __HAL_RCC_GPIOD_CLK_ENABLE();
  __HAL_RCC_GPIOC_CLK_ENABLE();
  __HAL_RCC_GPIOB_CLK_ENABLE();

  /*Configure GPIO pin Output Level */
  HAL_GPIO_WritePin(GPIOC, GPIO_PIN_9, GPIO_PIN_RESET);

  /*Configure GPIO pin Output Level */
  HAL_GPIO_WritePin(GPIOA, GPIO_PIN_8|GPIO_PIN_9|GPIO_PIN_10, GPIO_PIN_RESET);

  /*Configure GPIO pin Output Level */
  HAL_GPIO_WritePin(GPIOB, GPIO_PIN_3, GPIO_PIN_RESET);

  /*Configure GPIO pin : PC9 */
  GPIO_InitStruct.Pin = GPIO_PIN_9;
  GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_PP;
  GPIO_InitStruct.Pull = GPIO_NOPULL;
  GPIO_InitStruct.Speed = GPIO_SPEED_FREQ_LOW;
  HAL_GPIO_Init(GPIOC, &GPIO_InitStruct);

  /*Configure GPIO pins : PA8 PA9 PA10 */
  GPIO_InitStruct.Pin = GPIO_PIN_8|GPIO_PIN_9|GPIO_PIN_10;
  GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_PP;
  GPIO_InitStruct.Pull = GPIO_NOPULL;
  GPIO_InitStruct.Speed = GPIO_SPEED_FREQ_LOW;
  HAL_GPIO_Init(GPIOA, &GPIO_InitStruct);

  /*Configure GPIO pin : PB3 */
  GPIO_InitStruct.Pin = GPIO_PIN_3;
  GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_PP;
  GPIO_InitStruct.Pull = GPIO_NOPULL;
  GPIO_InitStruct.Speed = GPIO_SPEED_FREQ_LOW;
  HAL_GPIO_Init(GPIOB, &GPIO_InitStruct);

  /* USER CODE BEGIN MX_GPIO_Init_2 */
  /* EXTI Interrupt Init */
  //HAL_NVIC_SetPriority(EXTI0_IRQn, 1, 0);
  //HAL_NVIC_EnableIRQ(EXTI0_IRQn);

  //HAL_NVIC_SetPriority(EXTI1_IRQn, 1, 0);
  //HAL_NVIC_EnableIRQ(EXTI1_IRQn);

  /* USER CODE END MX_GPIO_Init_2 */
}

/* USER CODE BEGIN 4 */
void HAL_TIM_PeriodElapsedCallback(TIM_HandleTypeDef *htim)
{
	if (htim->Instance == TIM3) // Đảm bảo đúng Timer PID
	  {

		  //mpu6050_read_accel(&accel);
		  //mpu6050_read_gyro(&gyro);
		  //Gyro_Y_Rate = gyro.y_val;
		  //Angle_Pitch = 0.98f * (Angle_Pitch + Gyro_Y_Rate * dt) + 0.02f * accel.pitch_angle;

		  mpu6050_read_all(&accel, &gyro);
		  mpu6050_complementary_filter(&gyro, &accel, &angle, dt);
		  Angle_Pitch = angle.pitch;

	      // Đọc Encoder
	      enc1_new = (int16_t)__HAL_TIM_GET_COUNTER(&htim4);
	      enc2_new = (int16_t)__HAL_TIM_GET_COUNTER(&htim1);

	      // Reset về 0 để lấy vận tốc tức thời
	      //__HAL_TIM_SET_COUNTER(&htim4, 0);
	      //__HAL_TIM_SET_COUNTER(&htim1, 0);
	      int16_t delta1 = enc1_new - enc1_old;
	      int16_t delta2 = enc2_new - enc2_old;

		  // Cập nhật giá trị cũ cho vòng lặp sau
		  enc1_old = enc1_new;
		  enc2_old = enc2_new;
		  //MPU6050ReadG();
		  //MPU6050ReadA();
		  //MPU6050_getAngle(AX, AY, AZ, GX, GY, GZ);
		  //MPU6050_filter(pitch, GX, dt);
		  cnt++;
		  if(cnt == 300)
		  {
			   Kp_ang = 60.0f;
			   Ki_ang = 0.005f;
			   Kd_ang = 1.2;

			   Kp_spd = 5.0f;
			   Ki_spd = 0.005f;
			   Setpoint_Angle = 8.0f;
		  }


	      Error_Ang = Setpoint_Angle - Angle_Pitch;

	      float P_Ang = Kp_ang * Error_Ang;

	      I_Part_Ang += (Ki_ang * Error_Ang * dt);
	      if (I_Part_Ang > 350) I_Part_Ang = 350;      // Anti-windup
	      else if (I_Part_Ang < -350) I_Part_Ang = -350;

	      float D_Ang = Kd_ang * (Error_Ang - Prev_Error_Ang) / dt;
	      Prev_Error_Ang = Error_Ang;

	      Target_Speed = P_Ang + I_Part_Ang + D_Ang;

	      if (Target_Speed > 7000) Target_Speed = 7000;
	      else if (Target_Speed < -7000) Target_Speed = -7000;

	      //static int16_t enc1_old = 0;
	      //static int16_t enc2_old = 0;

	      Encoder_Speed = ((float)delta1 + (float)delta2) / 2.0f / dt;

	      //v_filter = 0.85f * v_filter + 0.15f * Encoder_Speed;

	      Error_Spd = Target_Speed - Encoder_Speed;

	      float P_Spd = Kp_spd * Error_Spd;

	      I_Part_Spd += (Ki_spd * Error_Spd * dt);

	      if (I_Part_Spd > 4000) I_Part_Spd = 4000;
	      else if (I_Part_Spd < -4000) I_Part_Spd = -4000;

	      PWM_Out = P_Spd + I_Part_Spd;

	      if (PWM_Out > 0) PWM_Out += 250;
	      else if (PWM_Out < 0) PWM_Out -= 150;

	      // Safety Stop
		  if (fabs(Angle_Pitch) > 45.0f || fabs(Angle_Pitch) < -45.0f) {
			  PWM_Out = 0;
			  I_Part_Spd = 0;
			  I_Part_Ang = 0;
		  }



	      Motor_Control(PWM_Out);

	      //timer_flag = 1;
	      //int len = sprintf(usb_buffer, "%.2f,%.2f,%.2f\r\n", Angle_Pitch, 1000.0f, v_filter);
	      //CDC_Transmit_FS((uint8_t*)usb_buffer, len);

	      /*
	      char usb_buffer[64];
		  static uint8_t count_debug = 0;
		  count_debug++;
		  if (count_debug >= 10) // 50ms gửi 1 lần
		  {

			  int len = sprintf(usb_buffer, "%d,%d,%.0f,%.0f,%.0f\r\n", enc1_new, enc2_new, Angle_Pitch, PWM_Out, Encoder_Speed);
			  CDC_Transmit_FS((uint8_t*)usb_buffer, len);
			  count_debug = 0;
			  //send_usb_flag = 1;

		  }
		  */
		  //memset(usb_buffer, 0, sizeof(usb_buffer));
		  //int len = sprintf(usb_buffer, "%.2f,%.2f,%.2f\r\n", Angle_Pitch, 1000.0f, v_filter);
		  //int len = snprintf(usb_buffer, sizeof(usb_buffer), "%d,%d,%.0f,%.0f,%.0f\r\n", enc1_new, enc2_new, Angle_Pitch, PWM_Out, Encoder_Speed);
	      //timer_flag = 1;
	  }
}

